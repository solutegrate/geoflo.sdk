{
	"plugins": [],
	"recurseDepth": 10,
	"opts": {
    "recurse": true,
    "lenient": false,
    "sort": true,
    "readme": "./README.md",
    "encoding": "utf8",
    "destination": "./docs/",
    "template": "./node_modules/tui-jsdoc-template",
    "package": "./package.json",
    "tutorials": "./docs/tutorials"
  },
	"source": {
		"include": [ "./index.js", "./src/Options.js" ],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|/)node_modules/"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": [
			"jsdoc",
			"closure"
		]
	},
	"templates": {
		"name": "Documentation",
		"footerText": "Solutegrate/GeoFlo Â© 2025",
		"includeSource": false,
		"systemName": "GeoFlo",
		"navType": "vertical",
		"linenums": true,
		"dateFormat": "MMMM Do YYYY, h:mm:ss a",
		"useCollapsibles": true,
		"logo": {
			"url": "https://geoflo.s3.amazonaws.com/logos/logo_full_white.png",
			"link": "https://docs.geoflo.pro"
		},
		"css": [
			"./styles/custom.css"
		],
		"tabNames": {
			"api": "SDK",
			"tutorials": "Examples"
		},
		"better-docs": {
			"entryPoint": "README.md",
			"includeSource": false,
			"name": "",
			"title": "GeoFlo SDK",
			"logo": "https://geoflo.s3.amazonaws.com/logos/logo_full_white.png",
			"css": [
				"./styles/custom.css"
			],
			"navigation": [
				{
					"label": "Home",
					"href": "index.html"
				},
				{
					"label": "Examples",
					"href": "tutorials.html"
				}
			]
		}
	}
}