    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "better-docs": "^2.3.2",
    "jsdoc": "^4.0.2",
    "webpack-obfuscator": "^3.6.0"
  },
  "peerDependencies": {
    "mapbox-gl": "^3.3.0"
